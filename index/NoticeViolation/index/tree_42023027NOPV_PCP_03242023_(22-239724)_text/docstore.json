cluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "a156f365-d909-4d89-a268-360d75829342", "node_type": "4", "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_name": "42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_type": "text/plain", "file_size": 7271, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "hash": "601097b1cd0c44fef6a28b34a3b122866360532b2b961823334d6a8beea51cf8", "class_name": "RelatedNodeInfo"}, "2": {"node_id": "885e78a6-2b3e-4da0-80c2-b5d333eee9f6", "node_type": "1", "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_name": "42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_type": "text/plain", "file_size": 7271, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "hash": "4b9581a99d5efff30594143f8016013369057ef43306d44d843193a60087b9cc", "class_name": "RelatedNodeInfo"}, "3": {"node_id": "a3d4d1ba-4bf5-40f4-92b6-aa8a86473874", "node_type": "1", "metadata": {}, "hash": "2afe011be59d00fbb20ab096a08b393b3fc67b24cd85631c580993ecfa2901a4", "class_name": "RelatedNodeInfo"}}, "hash": "579a00dae326e395bfc44ba93b4d80f005b2592a7e512867c6e5369c9dcb3a91", "text": "\u00a7 552(b), along with the complete original document you must provide a second copy of \nthe  document  with  the  portions  you  believe  qualify  for  confidential  treatment  redacted  and  an \nexplanation of why you believe the redacted information qualifies for confidential treatment under \n5 U.S.C. \u00a7 552(b). \n\nFollowing the receipt of this Notice, you have 30 days to submit written comments, or request a \nhearing under 49 CFR \u00a7 190.211. If you do not respond within 30 days of receipt of this Notice, \nthis constitutes a waiver of your right to contest the allegations in this Notice and authorizes the \nAssociate Administrator for Pipeline Safety to find facts as alleged in this Notice without further \nnotice to you and to issue a Final Order. If you are responding to this Notice, we propose that you \nsubmit your correspondence to my office within 30 days from the receipt of this Notice.", "start_char_idx": 5844, "end_char_idx": 6745, "text_template": "{metadata_str}\n\n{content}", "metadata_template": "{key}: {value}", "metadata_seperator": "\n", "class_name": "TextNode"}, "__type__": "1"}, "a3d4d1ba-4bf5-40f4-92b6-aa8a86473874": {"__data__": {"id_": "a3d4d1ba-4bf5-40f4-92b6-aa8a86473874", "embedding": null, "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_name": "42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_type": "text/plain", "file_size": 7271, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "a156f365-d909-4d89-a268-360d75829342", "node_type": "4", "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_name": "42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_type": "text/plain", "file_size": 7271, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "hash": "601097b1cd0c44fef6a28b34a3b122866360532b2b961823334d6a8beea51cf8", "class_name": "RelatedNodeInfo"}, "2": {"node_id": "f37c6322-ef9a-4894-987a-d18e6ba6a2e0", "node_type": "1", "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_name": "42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_type": "text/plain", "file_size": 7271, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "hash": "579a00dae326e395bfc44ba93b4d80f005b2592a7e512867c6e5369c9dcb3a91", "class_name": "RelatedNodeInfo"}}, "hash": "2afe011be59d00fbb20ab096a08b393b3fc67b24cd85631c580993ecfa2901a4", "text": "This \nperiod may be extended by written request for good cause. \n\nIn  your  correspondence  on  this  matter,  please  refer  to  CPF  4-2023-027-NOPV,  and  for  each \ndocument you submit, please provide a copy in electronic format whenever possible. \n\n \n \n \n \n \n \n\fSincerely, \n\nBryan Lethcoe \nDirector, Southwest Region, Office of Pipeline Safety \nPipeline and Hazardous Materials Safety Administration \n\nEnclosure:  Response Options for Pipeline Operators in Enforcement Proceedings", "start_char_idx": 6747, "end_char_idx": 7232, "text_template": "{metadata_str}\n\n{content}", "metadata_template": "{key}: {value}", "metadata_seperator": "\n", "class_name": "TextNode"}, "__type__": "1"}, "29bdde27-d3d5-403a-bd16-eefa00012739": {"__data__": {"id_": "29bdde27-d3d5-403a-bd16-eefa00012739", "embedding": null, "metadata": {}, "excluded_embed_metadata_keys": [], "excluded_llm_metadata_keys": [], "relationships": {}, "hash": "c50e024425d2fc8b33bbea31aa797aaf364c77120a3e992b1746b7123a3ed90f", "text": "The Pipeline and Hazardous Materials Safety Administration (PHMSA) has issued a Notice of Probable Violation and Proposed Civil Penalty to Sea Robin Pipeline Company. The notice alleges that Sea Robin failed to provide protection against atmospheric corrosion found during an inspection of their Offshore Pipeline System in the Gulf of Mexico and Louisiana, in violation of Pipeline Safety Regulations. Specifically, Sea Robin failed to remediate atmospheric corrosion found at the Vermillion 149 platform within the required timeframe. The company is subject to a civil penalty not to exceed $257,664 per violation per day the violation persists, up to a maximum of $2,576,627 for a related series of violations. The PHMSA has preliminarily assessed a civil penalty of $39,400. Sea Robin has 30 days to submit written comments or request a hearing.", "start_char_idx": null, "end_char_idx": null, "text_template": "{metadata_str}\n\n{content}", "metadata_template": "{key}: {value}", "metadata_seperator": "\n", "class_name": "TextNode"}, "__type__": "1"}, "7dad692e-3ee1-4356-926d-cf23641bf3da": {"__data__": {"id_": "7dad692e-3ee1-4356-926d-cf23641bf3da", "embedding": null, "metadata": {}, "excluded_embed_metadata_keys": [], "excluded_llm_metadata_keys": [], "relationships": {}, "hash": "4d3f656e330cc92888782e319ab32c8e8033479ec6f4ee676a95cad046b92ca4", "text": "The document is a notice of violation from the Office of Pipeline Safety, Pipeline and Hazardous Materials Safety Administration, signed by Bryan Lethcoe, Director of the Southwest Region. The notice allows for an extension period upon written request for good cause. All correspondence should refer to CPF 4-2023-027-NOPV and should be provided in electronic format whenever possible. The document also includes an enclosure detailing response options for pipeline operators in enforcement proceedings.", "start_char_idx": null, "end_char_idx": null, "text_template": "{metadata_str}\n\n{content}", "metadata_template": "{key}: {value}", "metadata_seperator": "\n", "class_name": "TextNode"}, "__type__": "1"}}, "docstore/ref_doc_info": {"a156f365-d909-4d89-a268-360d75829342": {"node_ids": ["babbd391-674e-4730-aa2d-95adfcadf432", "3d866a6e-0802-4e48-9e4a-acb737dd857c", "c0919a19-5be6-433a-8bf6-251651bd8691", "ecbba194-e586-4367-837d-d1656f9e838b", "43fbb030-8d1c-4a8e-95a2-4a9cb18754fe", "a4ef7d3d-d11b-4307-b7dd-e5f1b6983818", "00a316a6-aa00-43d0-b2e9-9d448aaedfcb", "be665424-5649-427d-8c2d-bb15b7d7b99c", "885e78a6-2b3e-4da0-80c2-b5d333eee9f6", "f37c6322-ef9a-4894-987a-d18e6ba6a2e0", "a3d4d1ba-4bf5-40f4-92b6-aa8a86473874"], "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_name": "42023027NOPV_PCP_03242023_(22-239724)_text.txt", "file_type": "text/plain", "file_size": 7271, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}}}}