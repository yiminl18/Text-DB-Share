adata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/12023028NOPV_PCO_05252023_(22-232998)_text.txt", "file_name": "12023028NOPV_PCO_05252023_(22-232998)_text.txt", "file_type": "text/plain", "file_size": 8875, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "excluded_embed_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "excluded_llm_metadata_keys": ["file_name", "file_type", "file_size", "creation_date", "last_modified_date", "last_accessed_date"], "relationships": {"1": {"node_id": "2b01e8df-430f-4819-9519-8c8eaa9b95ef", "node_type": "4", "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/12023028NOPV_PCO_05252023_(22-232998)_text.txt", "file_name": "12023028NOPV_PCO_05252023_(22-232998)_text.txt", "file_type": "text/plain", "file_size": 8875, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "hash": "5358088a4c7f15cb6b097fad71a7e2da8de14f5c21d1cacc881a7a12ffcbc758", "class_name": "RelatedNodeInfo"}, "2": {"node_id": "f9d12764-2f16-4133-8106-92560614d2a0", "node_type": "1", "metadata": {"file_path": "/Users/yiminglin/Documents/Codebase/TextDB/Text-DB/data/NoticeViolation/extracted_data/12023028NOPV_PCO_05252023_(22-232998)_text.txt", "file_name": "12023028NOPV_PCO_05252023_(22-232998)_text.txt", "file_type": "text/plain", "file_size": 8875, "creation_date": "2024-03-29", "last_modified_date": "2024-03-29", "last_accessed_date": "2024-03-29"}, "hash": "e6002c78634091961141803f3e591248cb971b8d172b619d0402aeb81abd5881", "class_name": "RelatedNodeInfo"}, "3": {"node_id": "e37d24e4-6912-436f-adc5-dd4568091f95", "node_type": "1", "metadata": {}, "hash": "233db8bb38670a98a068b42818d94c6db406cc3352a91964c900934e3c50ccf1", "class_name": "RelatedNodeInfo"}}, "hash": "8b842db1448